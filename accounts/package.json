{
  "name": "accounts",
  "version": "1.0.0",
  "description": "User Account Management API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "tsnd --respawn --transpile-only src/server.ts",
    "test": "npm-run-all test:contract test:integration test:unit",
    "test:unit": "jest --passWithNoTests --runInBand --verbose=true test/unit/",
    "test:integration": "jest --passWithNoTests --runInBand --verbose=true test/integration/",
    "test:contract": "jest --passWithNoTests --runInBand --verbose=true test/contract/"
  },
  "author": "Tony Westerich",
  "license": "ISC",
