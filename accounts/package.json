    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "joi": "^17.12.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "winston": "3.11.0",
    "winston-syslog": "^2.7.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
