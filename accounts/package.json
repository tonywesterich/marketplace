    "@types/winston": "^2.4.4",
    "@types/winston-syslog": "^2.4.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
